{
    "version": 2,
    "build": {
        "env": {
            "NODE_PATH": "./src"
        }
    },
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ]
}