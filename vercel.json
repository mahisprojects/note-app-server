{
    "version": 2,
    "env": {
        "NODE_PATH": "src"
    },
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ]
}